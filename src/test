fn fibonacci(n) {
    if(n == 0 or n == 1) {
        return n
    }
    else(True) {
        return fibonacci(n - 1) + fibonacci(n - 2)
    }
    return 1
}

a = -2

fn some_loop (x, y) {
    while (True) {
        y = 2 *= (- (++x))
    }
    return y + (x * 2)
}

fn bajojajo() {}

fn print(){}

fn main() {
    *x = -10
    print(fibonacci(-x))
}